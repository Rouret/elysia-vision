{
  "name": "elysia-vision",
  "version": "0.1.1",
  "description": "Plugin for Elysia that monitors your API requests in real-time through a web dashboard.",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "rouret",
    "url": "https://github.com/rouret"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rouret/elysia-vision"
  },
  "homepage": "https://github.com/rouret/elysia-vision",
  "keywords": [
    "elysia",
    "vision"
  ],
  "license": "MIT",
  "scripts": {
    "test": "bun test --watch",
    "dev": "bun --hot testing/local.ts",
    "release": "bun --hot testing/release.ts",
    "fakeClient": "bun scripts/fakeClient.ts",
    "build": "bun build.ts"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "elysia": "^1.4.19",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "elysia": ">= 1.4.0"
  }
}
